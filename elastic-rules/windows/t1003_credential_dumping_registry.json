{
  "name": "Credential Dumping via Registry Hive Access",
  "description": "Detects attempts to dump credentials by accessing Windows registry hives using reg.exe (T1003.002)",
  "type": "query",
  "language": "kuery",
  "query": "process.name : \"reg.exe\" AND registry.hive : \"HKLM\"",
  "risk_score": 73,
  "severity": "high",
  "enabled": false,
  "interval": "5m",
  "from": "now-6m",
  "index": [
    "logs-*",
    "winlogbeat-*",
    "filebeat-*",
    "endgame-*"
  ],
  "tags": [
    "Domain: Endpoint",
    "OS: Windows",
    "Use Case: Threat Detection",
    "Tactic: Credential Access",
    "Data Source: Process",
    "MITRE ATT&CK: T1003.002"
  ]
}