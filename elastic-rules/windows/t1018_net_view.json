{
  "name": "Remote System Discovery via Net View",
  "description": "Detects usage of net.exe view command for remote system discovery (T1018)",
  "type": "query",
  "language": "kuery",
  "query": "event.code: \"1\" and process.executable: *net.exe and process.command_line: *view*",
  "risk_score": 47,
  "severity": "low",
  "enabled": false,
  "interval": "5m",
  "from": "now-6m",
  "index": [
    "logs-*",
    "winlogbeat-*",
    "filebeat-*",
    "endgame-*"
  ],
  "tags": [
    "Domain: Endpoint",
    "OS: Windows",
    "Use Case: Threat Detection",
    "Tactic: Discovery",
    "Data Source: Process",
    "MITRE ATT&CK: T1018"
  ]
}